{
	"name": "portfolio",
	"version": "0.0.1",
	"engines": {
		"node": "19.6.0"
	},
	"private": true,
	"scripts": {
		"dev": "vite --host --port 3504",
		"start": "vite preview --host",
		"build": "svelte-kit sync && vite build",
		"preview": "vite preview --host --port 4504",
		"sync": "svelte-kit sync && pnpm-lock-export && npm i --package-lock-only",
		"sync:up": "pnpm update && svelte-kit sync && pnpm-lock-export && npm i --package-lock-only",
		"check": "svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"story:dev": "histoire dev --port 5504",
		"story:build": "histoire build",
		"story:preview": "histoire preview --port 6504"
	},
	"dependencies": {
		"@pnpm/logger": "^2.1.1",
		"@stefanprobst/remark-shiki": "2.1.0",
		"@sveltejs/adapter-node": "^1.1.7",
		"@sveltejs/kit": "^1.5.0",
		"@tailwindcss/typography": "^0.5.9",
		"@types/node": "^18.13.0",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"autoprefixer": "^10.4.13",
		"date-fns": "^2.29.3",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"gray-matter": "^4.0.3",
		"highlight.js": "^11.7.0",
		"imagetools-core": "^3.3.1",
		"js-base64": "^3.7.5",
		"mdsvex": "^0.10.6",
		"node-fetch": "2.6.7",
		"node-html-parser": "^6.1.4",
		"parse-link-header": "^2.0.0",
		"path-browserify": "^1.0.1",
		"pnpm-lock-export": "^0.4.0",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-document": "^6.1.0",
		"rehype-format": "^4.0.1",
		"rehype-parse": "^8.0.4",
		"rehype-raw": "^6.1.1",
		"rehype-slug": "^5.1.0",
		"rehype-stringify": "^9.0.3",
		"remark-abbr": "^1.4.1",
		"remark-frontmatter": "^4.0.1",
		"remark-gfm": "^3.0.1",
		"remark-github": "^11.2.4",
		"remark-parse": "^10.0.1",
		"remark-rehype": "^10.1.0",
		"remark-sectionize": "^1.1.1",
		"remark-toc": "^8.0.1",
		"rss": "^1.2.2",
		"shiki": "0.10.1",
		"slugify": "^1.6.5",
		"snarkdown": "^2.0.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-highlight": "^6.2.1",
		"svelte-icons": "^2.1.0",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.6",
		"to-vfile": "^7.2.4",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"unified": "^10.1.2",
		"vfile": "^5.3.6",
		"vfile-reporter": "^7.0.5",
		"vite": "^4.1.1",
		"vite-imagetools": "^4.0.18"
	},
	"type": "module",
	"devDependencies": {
		"@histoire/plugin-svelte": "^0.12.4",
		"histoire": "^0.12.4"
	}
}
