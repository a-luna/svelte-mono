{
	"name": "@a-luna/shared-ui",
	"version": "0.0.1",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/a-luna/svelte-mono.git",
		"directory": "packages/shared-ui"
	},
	"engines": {
		"node": "^20.0.0"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"sync": "svelte-kit sync",
		"sync:up": "pnpm --filter shared-ui update && svelte-kit sync && pnpm-lock-export && npm i --package-lock-only && find . -name '.DS_Store' -type f -delete",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"dependencies": {
		"@sveltejs/adapter-auto": "^2.0.1",
		"@sveltejs/kit": "^1.16.3",
		"@sveltejs/package": "^2.0.2",
		"svelte": "^3.59.1",
		"vite": "^4.3.5",
		"vitest": "^0.29.8"
	},
	"devDependencies": {
		"@types/node": "^18.16.9",
		"@typescript-eslint/eslint-plugin": "^5.59.5",
		"@typescript-eslint/parser": "^5.59.5",
		"@vitest/coverage-c8": "^0.29.8",
		"@vitest/ui": "^0.29.8",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte-check": "^3.3.2",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4"
	},
	"type": "module",
	"publishConfig": {
		"directory": "dist"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.js",
	"svelte": "./dist/index.js",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./package.json": "./package.json",
		"./dist": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		}
	}
}
