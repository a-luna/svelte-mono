{
	"name": "@a-luna/shared-ui",
	"version": "0.0.1",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/a-luna/svelte-mono.git",
		"directory": "packages/shared-ui"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"sync": "svelte-kit sync",
		"sync:up": "pnpm --filter shared-ui update && svelte-kit sync && pnpm-lock-export && npm i --package-lock-only && find . -name '.DS_Store' -type f -delete",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest run --coverage",
		"test:ui": "vitest watch --ui --threads false --coverage",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"dependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.24.0",
		"@sveltejs/package": "^2.2.2",
		"esm-env": "^1.0.0",
		"svelte": "^3.59.2",
		"vite": "^4.4.9"
	},
	"devDependencies": {
		"@types/node": "^18.17.12",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"@vitest/coverage-v8": "^0.33.0",
		"@vitest/ui": "^0.33.0",
		"eslint": "^8.48.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte-check": "^3.5.1",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vitest": "^0.33.0"
	},
	"type": "module",
	"publishConfig": {
		"directory": "dist"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.js",
	"svelte": "./dist/index.js",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./package.json": "./package.json",
		"./dist": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		}
	}
}
