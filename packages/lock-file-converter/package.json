{
	"name": "@a-luna/lock-file-converter",
	"version": "0.0.1",
	"license": "MIT",
	"author": "Aaron Luna <contact@aaronluna.dev>",
	"description": "Create dedicated package-lock.json files from monorepo project pnpm-lock.yaml files.",
	"main": "dist/src/index.js",
	"files": [
		"dist/"
	],
	"bin": "dist/src/bin/pnpm-lock-export.js",
	"scripts": {
		"build": "tsc",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/a-luna/svelte-mono.git",
		"directory": "packages/lock-file-converter"
	},
	"peerDependencies": {
		"svelte": "^3.54.0"
	},
	"dependencies": {
		"@manypkg/get-packages": "^1.1.3",
		"@pnpm/lockfile-file": "^5.3.8",
		"@pnpm/lockfile-types": "^4.3.6",
		"@pnpm/lockfile-utils": "^4.2.8",
		"@pnpm/logger": "^4.0.0",
		"@pnpm/prune-lockfile": "^4.0.24",
		"@pnpm/read-project-manifest": "^3.0.13",
		"@pnpm/registry-mock": "^2.20.0",
		"@pnpm/types": "^8.10.0",
		"@tsconfig/node16-strictest": "^1.0.4",
		"@types/argparse": "^2.0.10",
		"@types/node": "^16.18.38",
		"@types/semver": "^7.5.0",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.61.0",
		"argparse": "^2.0.1",
		"dependency-path": "^9.2.8",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"prettier": "^2.8.8",
		"ramda": "^0.28.0",
		"semver": "^7.5.3",
		"tmp-promise": "^3.0.3",
		"typescript": "^5.1.6"
	},
	"eslintConfig": {
		"parser": "@typescript-eslint/parser",
		"plugins": [
			"@typescript-eslint"
		],
		"extends": [
			"eslint:recommended",
			"plugin:@typescript-eslint/eslint-recommended",
			"plugin:@typescript-eslint/recommended"
		],
		"ignorePatterns": [
			"dist/"
		]
	}
}
