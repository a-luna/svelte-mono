{
	"name": "@a-luna/svelte-base64",
	"license": "MIT",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/a-luna/svelte-mono.git",
		"directory": "apps/svelte-base64"
	},
	"scripts": {
		"dev": "vite --host --port 3500",
		"build": "svelte-kit sync && vite build",
		"build:ci": "npx playwright install && vite build && vitest run",
		"build:prod": "svelte-kit sync && vite build --mode prod",
		"start": "vite preview",
		"preview": "svelte-kit preview --host",
		"sync": "svelte-kit sync",
		"sync:up": "pnpm --filter @a-luna/svelte-base64 update && svelte-kit sync && pnpm-lock-export && npm i --package-lock-only && find . -name '.DS_Store' -type f -delete",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest run --coverage",
		"test:ui": "vitest watch --ui --threads false --coverage",
		"test:watch": "vitest watch --coverage",
		"svelte:check": "svelte-check --fail-on-warnings",
		"prettier:check": "prettier --check --plugin-search-dir=. .",
		"format": "prettier --write ./src/**/*.{js,svelte,html,ts}",
		"prelint": "npm run format",
		"lint": "eslint './src/**/*.{js,ts,svelte}'",
		"lint:fix": "eslint --fix './src/**/*.{js,ts,svelte}'"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.8.1",
		"@playwright/test": "^1.41.1",
		"@testing-library/dom": "^8.20.1",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/svelte": "^3.2.2",
		"@testing-library/user-event": "^14.5.2",
		"@types/cookie": "^0.5.4",
		"@types/node": "^18.19.10",
		"@types/testing-library__jest-dom": "^5.14.9",
		"@typescript-eslint/eslint-plugin": "^6.19.1",
		"@typescript-eslint/parser": "^6.19.1",
		"@vitest/coverage-v8": "^0.33.0",
		"@vitest/ui": "^0.33.0",
		"@xstate/cli": "^0.3.3",
		"@xstate/graph": "^1.4.2",
		"@xstate/test": "^0.5.1",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-svelte": "^2.35.1",
		"jsdom": "^20.0.3",
		"playwright": "^1.41.1",
		"postcss": "^8.4.33",
		"postcss-html": "^1.6.0",
		"postcss-load-config": "^4.0.2",
		"prettier": "^3.2.4",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.9",
		"svelte-check": "^3.6.3",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"ws": "^8.16.0"
	},
	"type": "module",
	"dependencies": {
		"@a-luna/shared-ui": "workspace:*",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^2.5.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@xstate/inspect": "^0.7.1",
		"@xstate/svelte": "^2.1.0",
		"cssnano": "^5.1.15",
		"vite": "^5.0.12",
		"vitest": "^0.33.0",
		"xstate": "^4.38.3",
		"zod": "^3.22.4"
	}
}
