	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/a-luna/svelte-mono.git",
		"directory": "apps/portfolio"
	},
	"engines": {
		"node": "^18.0.0"
	},
	"private": true,
	"scripts": {
		"dev": "vite --host --port 3504 --force",
		"start": "vite preview --host",
		"build": "svelte-kit sync && vite build",
		"preview": "vite preview --host --port 4504",
		"sync": "svelte-kit sync",
		"sync:up": "pnpm --filter portfolio update && svelte-kit sync && pnpm-lock-export && npm i --package-lock-only && find . -name '.DS_Store' -type f -delete",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --plugin-search-dir . --write .",
		"story:dev": "histoire dev --port 5504",
		"story:build": "histoire build",
		"story:preview": "histoire preview --port 6504"
	},
