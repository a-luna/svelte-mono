{
	"name": "svelte-base64",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite --host --port 3500",
		"build": "vite build",
		"build:ci": "npx playwright install && vite build && vitest run",
		"start": "vite preview",
		"preview": "svelte-kit preview --host",
		"sync": "svelte-kit sync && pnpm-lock-export && npm i --package-lock-only",
		"sync:up": "pnpm update && svelte-kit sync && pnpm-lock-export && npm i --package-lock-only",
		"test": "vitest run --coverage",
		"test:ui": "vitest watch --ui --threads false --coverage",
		"test:watch": "vitest watch --coverage",
		"svelte:check": "svelte-check --fail-on-warnings",
		"prettier:check": "prettier --check --plugin-search-dir=. .",
		"format": "prettier --write ./src/**/*.{js,svelte,html,ts}",
		"prelint": "npm run format",
		"lint": "eslint './src/**/*.{js,ts,svelte}'",
		"lint:fix": "eslint --fix './src/**/*.{js,ts,svelte}'"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.4.2",
		"@playwright/test": "^1.30.0",
		"@pnpm/logger": "^2.1.1",
		"@sveltejs/adapter-netlify": "^1.0.5",
		"@testing-library/dom": "^8.20.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.2",
		"@testing-library/user-event": "^14.4.3",
		"@types/cookie": "^0.5.1",
		"@types/node": "^18.11.19",
		"@types/testing-library__jest-dom": "^5.14.5",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"@vitest/coverage-c8": "^0.26.3",
		"@vitest/ui": "^0.26.3",
		"@xstate/cli": "^0.3.3",
		"@xstate/graph": "^1.4.2",
		"@xstate/svelte": "^2.0.1",
		"@xstate/test": "^0.5.1",
		"autoprefixer": "^10.4.13",
		"c8": "^7.12.0",
		"cssnano": "^5.1.14",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.3",
		"jsdom": "^20.0.3",
		"lint-staged": "^13.1.0",
		"playwright": "^1.30.0",
		"pnpm-lock-export": "^0.4.0",
		"postcss": "^8.4.21",
		"postcss-html": "^1.5.0",
		"postcss-load-config": "^4.0.1",
		"pre-commit": "^1.2.2",
		"prettier": "^2.8.3",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"svelte-icons": "^2.1.0",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.1",
		"vitest": "^0.26.3",
		"ws": "^8.12.0",
		"xstate": "^4.35.4"
	},
	"type": "module",
	"engines": {
		"node": ">= 12.17.0"
	},
	"dependencies": {
		"@sveltejs/kit": "^1.5.0",
		"@xstate/inspect": "^0.7.1",
		"svelte-preprocess": "^4.10.7"
	}
}
